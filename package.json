  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.5",
    "@emotion/babel-plugin": "^11.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "~5.7.2",
    "vite": "^6.2.0"
  }
}
